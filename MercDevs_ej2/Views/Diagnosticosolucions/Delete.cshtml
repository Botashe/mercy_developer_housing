@model Diagnosticosolucion

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>¿Estas seguro de que quieres eliminar esto?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DescripcionDiagnostico)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DescripcionDiagnostico)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DescripcionSolucion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DescripcionSolucion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DatosFichaTecnica)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DatosFichaTecnica.IdDatosFichaTecnica)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdDiagnosticoSolucion" />
        <input type="submit" value="Borrar Diagnostico" class="btn btn-danger" />        
    </form>
    <a asp-action="Index"><button class="btn btn-outline-secondary">Volver a la Lista</button></a>
</div>
